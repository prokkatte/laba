#include <stdio.h>

int main() {
    char str[100];
    int count = 0;
    
    printf("Введите строку: ");
    fgets(str, sizeof(str), stdin);
    
    for (int i = 0; str[i] != '\0'; i++) {
        if (str[i] >= '0' && str[i] <= '9') {
            count++;
        }
    }
    
    printf("Количество цифр в строке: %d\n", count);
    
    return 0;
